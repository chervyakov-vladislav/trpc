    "lodash": "^4.17.21",
    "superjson": "^2.2.2",
    "trpc-playground": "^1.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.13",
    "prisma": "^6.1.0"
  }
